

@model IEnumerable<VideoRentalProject.Models.Movie>
@{
    ViewBag.Title = "Index";
}

<h2>Customers</h2>
<ul>
    @if (Model.Count() == 0)
    {
        <p>
            There is no Movie in the List
        </p>
    }
</ul>
@foreach (var c in Model)
{
    <li> @Html.ActionLink(c.Name, "Details", "Movies", new { id = c.id }, null)</li>

}

